{
  "name": "APOLLO",
  "description": "Advanced Phishing preventiOn with Large Language model-based Oracle",
  "version": "1.0",
  "manifest_version": 3,
  "key": "",
  "permissions": [
    "storage",
    "identity",
    "tabs"
  ],
  "oauth2": {
    "client_id": "937476424219-gp0dchru70bqrts9tj0g594v2momun0o.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.readonly",
      "email",
      "profile",
      "openid"
    ]
  },
  "action": {
    "default_popup": "popup/hello.html"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://mail.google.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "host_permissions": [
    "http://127.0.0.1:5000/*"
  ],
  "options_page": "options/options.html",
  "icons": {
   "16": "images/apollo_icon16.png",
   "32": "images/apollo_icon32.png",
   "48": "images/apollo_icon48.png",
   "128": "images/apollo_icon128.png"
  }
}